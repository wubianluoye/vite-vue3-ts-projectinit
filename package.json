{
  "name": "vue3-ts-project",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --open --mode development",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:lint-staged": "npx lint-staged",
    "lint": "eslint --fix . && prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}